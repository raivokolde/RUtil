draw_matrix = function(matrix, border_color){#
	n = nrow(matrix)#
	m = ncol(matrix)#
	x = (1:m)/m - 1/2/m#
	y = (1:n)/n - 1/2/n#
	for(i in 1:m){#
		grid.rect(x = x[i], y = y[1:n], width = 1/m, height = 1/n, gp = gpar(fill = matrix[,i], col = border_color))#
	}#
}#
